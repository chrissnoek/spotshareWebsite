{
  "kind": "collectionType",
  "collectionName": "photos",
  "info": {
    "name": "Photo",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "desc": {
      "type": "richtext"
    },
    "photo": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "photos"
    },
    "date": {
      "type": "datetime"
    },
    "brand": {
      "type": "string"
    },
    "shutterspeed": {
      "type": "string"
    },
    "camera": {
      "type": "string"
    },
    "likes": {
      "type": "integer"
    },
    "location": {
      "model": "location",
      "via": "photos"
    },
    "iso": {
      "type": "string"
    },
    "aperture": {
      "type": "string"
    },
    "focalLength": {
      "type": "string"
    },
    "photo_categories": {
      "collection": "photo-category",
      "via": "photos"
    },
    "usersLike": {
      "plugin": "users-permissions",
      "collection": "user",
      "via": "likedPhotos"
    },
    "comments": {
      "collection": "photo-comment",
      "via": "photo"
    },
    "notifications": {
      "collection": "notification",
      "via": "photo"
    },
    "featured": {
      "type": "boolean"
    }
  }
}
